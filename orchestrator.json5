{
  devDependenciesLocation: "orchestrator-deps",
  processes: [
    {
      name: "3db",
      source: "3sig/3suite-db",

      sourceActions: [{ type: "chmod" }],


      // uncomment the following lines to use the local build
      //sourceType: "local",
      //localPath: "../../3suite/3suite-db",


      config: {
        verbose: true,
        plugins: [
            {
                name: "socketio",
                path: "3db/socketio.js",

                config: {
                    serverUrl: "localhost:6000",
                    socketioPrefix: "3db/"
                }
            }
        ]
      }
    },
    {
        name: "3suite-socketio",
        source: "3sig/3suite-socketio",
        sourceActions: [{ type: "chmod" }],
        config: {
          port: 6000,
        }
    },
    {
        name: "socketio",
        sourceType: "local",
        localPath: "./build",
        sourceFileType: "pattern-match",
        sourceFilePattern: "main.js",
        sourceExclude: true,
        sourceActions: [
        // update naming for your plugin here
          { type: "move", location: "./3db", filename: "socketio.js" }
        ],
    },
  ]
}
